<plugin>

   <extension-point id="remoteObjectType" name="%remoteObjectType" schema="schema/remoteObjectType.exsd"/>
   <extension-point id="editorPreferences" name="%extension-point.name.0" schema="schema/editorPreferences.exsd"/>
   <extension-point id="dockBars" name="%dockBars.name" schema="schema/dockBars.exsd"/>
   <extension-point id="productCustomizer" name="%productCustomizer" schema="schema/productCustomizer.exsd"/>

   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            name="%editorPreferences"
            category="com.metamatrix.modeler.ui.preferences.modelerPreferencePage"
            class="com.metamatrix.ui.internal.preferences.EditorPreferencePage"
            id="com.metamatrix.ui.internal.preferences.editorPreferencePage">
      </page>   
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            name="%propertyPages.details.name"
            class="com.metamatrix.ui.internal.logview.LogEntryPropertyPage"
            id="com.metamatrix.ui.StatusDetails">
         <enabledWhen>
            <adapt
                  type="com.metamatrix.ui.internal.logview.LogEntry">
            </adapt></enabledWhen>
      </page>
      <page
            name="%propertyPages.session.name"
            class="com.metamatrix.ui.internal.logview.LogSessionPropertyPage"
            id="com.metamatrix.ui.StatusSession">
         <enabledWhen>
            <adapt
                  type="com.metamatrix.ui.internal.logview.LogEntry">
            </adapt></enabledWhen>
      </page>
   </extension>

</plugin>
