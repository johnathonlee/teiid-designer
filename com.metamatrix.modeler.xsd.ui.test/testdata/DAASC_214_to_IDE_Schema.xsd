<?xml version='1.0' encoding='UTF-8'?>

<xs:schema elementFormDefault='qualified' attributeFormDefault='unqualified' xmlns:xs='http://www.w3.org/2001/XMLSchema' targetNamespace='http://www.ustranscom.mil/p3i/lm/214A' xmlns='http://www.ustranscom.mil/p3i/lm/214A'>

	<xs:complexType name='EnvelopeType'>
	  <xs:sequence>
		<!-- IC Comment:  -->
		<!-- Mapping Comment: Unique identifing filename; will discuss with DAASC -->
		<xs:element name='Filname' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='2'/>
		      <xs:maxLength value='100'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA01 -->
		<!-- Mapping Comment: AUTHORIZATION QUALIFIER -->
		<xs:element name='AuthQual' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='2'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA02 -->
		<!-- Mapping Comment: AUTHORIZATION INFO -->
		<xs:element name='AuthInfo' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='10'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA03 -->
		<!-- Mapping Comment: SECURITY QUALIFIER -->
		<xs:element name='SecQual' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='2'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA04 -->
		<!-- Mapping Comment: SECURITY INFO -->
		<xs:element name='SecInfo' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='15'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA05 -->
		<!-- Mapping Comment: SENDER QUALIFIER -->
		<xs:element name='SenderQual' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='2'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA06 -->
		<!-- Mapping Comment: SENDER ID -->
		<xs:element name='SenderId' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='15'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA07 -->
		<!-- Mapping Comment: INTERCHANGE QUALIFIER -->
		<xs:element name='IntQual' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='2'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA08 -->
		<!-- Mapping Comment: RECEIVER ID -->
		<xs:element name='ReceiverId' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='15'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA09 -->
		<!-- Mapping Comment: INTERCHANGE DATE -->
		<xs:element name='IntDate' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='6'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA10 -->
		<!-- Mapping Comment: INTERCHANGE TIME -->
		<xs:element name='IntTime' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='4'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA11 -->
		<!-- Mapping Comment: STANDARDS ID -->
		<xs:element name='StandardsId' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='1'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA12 -->
		<!-- Mapping Comment: INTERCHANGE CONTROL VERSION ID -->
		<xs:element name='IntContVersionId' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='5'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA13 -->
		<!-- Mapping Comment: INTERCHANGE CONTROL NUMBER -->
		<xs:element name='IsaIntContNum' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='9'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA14 -->
		<!-- Mapping Comment: ACKNOWLEDGMENT REQUEST -->
		<xs:element name='AckReq' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='1'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA15 -->
		<!-- Mapping Comment: TEST INDICATOR -->
		<xs:element name='TestIndicator' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='1'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: ISA16 -->
		<!-- Mapping Comment: SUBELEMENT SEPARATOR -->
		<xs:element name='SubelementSeparator' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='2'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: GS01 -->
		<!-- Mapping Comment: FUNCTIONAL IDENTIFIER CODE -->
		<xs:element name='FunctIdentCode' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='2'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: GS02 -->
		<!-- Mapping Comment: APPLICATION SENDERS CODE -->
		<xs:element name='AppSendersCode' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='2'/>
		      <xs:maxLength value='15'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: GS03 -->
		<!-- Mapping Comment: APPLICATION RECEIVERS CODE -->
		<xs:element name='AppReceiversCode' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='2'/>
		      <xs:maxLength value='15'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: GS04 -->
		<!-- Mapping Comment: TRANSLATION DATE -->
		<xs:element name='TranslationDate' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='8'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: GS05 -->
		<!-- Mapping Comment: TRANSLATION TIME -->
		<xs:element name='CarrierTranslationTime' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='4'/>
		      <xs:maxLength value='8'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: GS06 -->
		<!-- Mapping Comment: GROUP CONTROL NUMBER -->
		<xs:element name='GsGroupContNum' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='9'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: GS07 -->
		<!-- Mapping Comment: X12 AGENCY CODE -->
		<xs:element name='X12AgencyCode' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='2'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: GS08 -->
		<!-- Mapping Comment: VERSION/RELEASE/INDUSTRY INTENDTIFIER CODE -->
		<xs:element name='EdiVersion' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='12'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: GE01 -->
		<!-- Mapping Comment: NUMBER OF TRANSACTION SETS -->
		<xs:element name='NumTransSets' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='8'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: GE02 -->
		<!-- Mapping Comment: GROUP CONTROL NUMBER -->
		<xs:element name='GeGroupContNum' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='9'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: IEA01 -->
		<!-- Mapping Comment: Functional Group Count -->
		<xs:element name='FunctGroupCode' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='5'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- IC Comment: IEA02 -->
		<!-- Mapping Comment: Interchange Control Number -->
		<xs:element name='IeaIntContNum' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='9'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<xs:element name='Header' maxOccurs='unbounded' type='HeaderType'/>
	  </xs:sequence>
	</xs:complexType> <!-- EnvelopeType -->

	<xs:complexType name='HeaderType'>
	  <xs:sequence>
		<!-- EDI Indexes: 2 -->
		<!-- IC Comment:  -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='TransactionSetIdentifierCode' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:length value='3'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- EDI Indexes: 3 -->
		<!-- IC Comment: Identifying control number that must be unique within the transaction set functional group assigned by the originator for a transaction set. The application and structure of the control number must be agreed upon between trading partners. (For example, so -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='TransactionSetControlNumber' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='4'/>
		      <xs:maxLength value='9'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- EDI Indexes: 5 -->
		<!-- IC Comment: SEGMENT CONDITION: Use of either B1001 or B1006 is MANDATORY. Only the carrier's PRO number will appear in B1001. Use B1006 if shipment is identified by air or rail waybill number. -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='CarrierProNumber' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='30'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- EDI Indexes: 6 -->
		<!-- IC Comment: In this data element, enter the Shipment ID generated by the shipper. This must be either a bill of lading number or a TCN (Transportation Control Number). If you enter a TCN here, you must create a separate 214 for each TCN included on a single bill of l -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='LadingBillOrShipmentIdentifier' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='30'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- EDI Indexes: 7 -->
		<!-- IC Comment:  -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='MovementScac' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='2'/>
		      <xs:maxLength value='4'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- EDI Indexes: 8 -->
		<!-- IC Comment: Required if B1006 is used. -->
		<!-- IC Comment: AW - Air Waybill Number -->
		<!-- IC Comment: WY - Waybill Number -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='WaybillQualifier' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='2'/>
		      <xs:maxLength value='3'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- EDI Indexes: 9 -->
		<!-- IC Comment: CONDITION: Use of either B1001 or B1006 is MANDATORY. Only -->
		<!-- IC Comment: the carrier's air or rail waybill number will appear in B1006. Use -->
		<!-- IC Comment: B1001 if shipment is identified by a carrier's PRO number. -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='WaybillIdentifier' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='30'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<xs:element name='ReferenceIdentifier' maxOccurs='unbounded' minOccurs='0' type='ReferenceIdentifierType'/>
		<!-- EDI Indexes: 18, 19 -->
		<!-- IC Comment:  -->
		<!-- Mapping Comment: Concat multiple occur -->
		<xs:element name='FreeFormText' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='250'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<xs:element name='Party' maxOccurs='unbounded' minOccurs='0' type='PartyType'/>
		<xs:element name='InterlineRouting' maxOccurs='unbounded' minOccurs='0' type='InterlineRoutingType'/>
		<xs:element name='StatusEvent' maxOccurs='unbounded' type='StatusEventType'/>
		<!-- EDI Indexes: 111 -->
		<!-- IC Comment: Total segments in this transaction set including the ST and SE segments. -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='TotalSegmentQuantity' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='10'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- EDI Indexes: 112 -->
		<!-- IC Comment: This data element ends the transaction set and should match the number that appears in the ST02 that begins the transaction set. -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='TrailerTransactionSetControlNumber' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='4'/>
		      <xs:maxLength value='9'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

	  </xs:sequence>
	</xs:complexType> <!-- HeaderType -->

	<xs:complexType name='ReferenceIdentifierType'>
	  <xs:sequence>
		<!-- EDI Indexes: 12, 15, 54 -->
		<!-- IC Comment: Code qualifying the Reference Identification -->
		<!-- IC Comment: [012] Additional Shipment Identifier Qualifier (DG 10) -->
		<!-- IC Comment: Use code value 'BM' for Commercial Bill of Lading as well as Government -->
		<!-- IC Comment: Bill of Lading. -->
		<!-- IC Comment: [015] Commodity Identification Qualifier (DG 10) -->
		<!-- IC Comment: BL Government Bill of Lading [012] -->
		<!-- IC Comment: BM Bill of Lading Number [012] -->
		<!-- IC Comment: C7 Contract Line Item Number [012] -->
		<!-- IC Comment: CH Customer catalog number [015] Customer catalog number -->
		<!-- IC Comment: CT Contract Number [012] Contract Number -->
		<!-- IC Comment: PG Product Group [015] Product Group -->
		<!-- IC Comment: PO Purchase Order Number [012] -->
		<!-- IC Comment: RQ Purchase Requisition Number [012] -->
		<!-- IC Comment: STR Standard Transportation Commodity Code (STCC) Replacement Code [015] -->
		<!-- IC Comment: TG Transportation Control Number (TCN)[012] -->
		<!-- IC Comment: IT - Internal Customer Number -->
		<!-- Mapping Comment: Store As-is -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='ReferenceQualifier' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='2'/>
		      <xs:maxLength value='3'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- EDI Indexes: 11, 14, 53 -->
		<!-- IC Comment:  -->
		<!-- IC Comment: Internal Customer Number -->
		<!-- Mapping Comment: Store As-is -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='ReferenceIdentifier' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='30'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- EDI Indexes: 16 -->
		<!-- IC Comment:  -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='ReferenceText' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='80'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

	  </xs:sequence>
	</xs:complexType> <!-- ReferenceIdentifierType -->

	<xs:complexType name='PartyType'>
	  <xs:sequence>
		<!-- EDI Indexes: 21, 37, 56 -->
		<!-- IC Comment: Code identifying an organizational entity, a physical location, property or an individual -->
		<!-- IC Comment: [021] Consignee Identifier Code (DG 70) -->
		<!-- IC Comment: [056] Party Who Signed Delivery Receipt Code (DG 71) -->
		<!-- IC Comment: [037] Shipper Identifier Code (DG 71) -->
		<!-- IC Comment: CN Consignee -->
		<!-- IC Comment: [021] Consignee -->
		<!-- IC Comment: N5 Party Who Signed the Delivery Receipt -->
		<!-- IC Comment: [056] Party Who Signed the Delivery Receipt -->
		<!-- IC Comment: SH Shipper -->
		<!-- IC Comment: [037] Shipper -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='EntityTypeCode' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='2'/>
		      <xs:maxLength value='3'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- EDI Indexes: 22, 38, 57 -->
		<!-- IC Comment: Free-form name -->
		<!-- IC Comment: [022] Consignee Name (DG 70) -->
		<!-- IC Comment: Carriers are encouraged to provide this data if it is available. It must be provided if an Identification Code is not provided at N103 and N104. -->
		<!-- IC Comment: [038] Shipper Name (DG 71) -->
		<!-- IC Comment: Carriers are encouraged to provide this data if it is available. It must be provided if an Identification Code is not provided at N103 and N104. -->
		<!-- IC Comment: [057] Party Who Signed Delivery Receipt (DG 71) -->
		<!-- IC Comment: Required to satisfy syntax. -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='EntityName' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='60'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- EDI Indexes: 23, 39 -->
		<!-- IC Comment: Code designating the system/method of code structure used for Identification Code (67) -->
		<!-- IC Comment: [023] Identification Code Qualifier (DG 70) -->
		<!-- IC Comment: Required if N104 is used. -->
		<!-- IC Comment: [039] Identification Code Qualifier (DG 71) -->
		<!-- IC Comment: Required if N104 is used. -->
		<!-- IC Comment: 1 D-U-N-S Number, Dun & Bradstreet -->
		<!-- IC Comment: [023] D-U-N-S Number -->
		<!-- IC Comment: [039] D-U-N-S Number -->
		<!-- IC Comment: 9 D-U-N-S+4, D-U-N-S Number with Four Character -->
		<!-- IC Comment: Suffix -->
		<!-- IC Comment: [023] D-U-N-S+4 -->
		<!-- IC Comment: [039] D-U-N-S+4 -->
		<!-- IC Comment: 10 Department of Defense Activity Address Code -->
		<!-- IC Comment: (DODAAC) -->
		<!-- IC Comment: [023] Department of Defense Activity Address Code -->
		<!-- IC Comment: (DODAAC) -->
		<!-- IC Comment: [039] Department of Defense Activity Address Code -->
		<!-- IC Comment: (DODAAC) -->
		<!-- IC Comment: 27 Government Bill Of Lading Office Code (GBLOC) -->
		<!-- IC Comment: [023] Government Bill Of Lading Office Code -->
		<!-- IC Comment: (GBLOC) -->
		<!-- IC Comment: [039] Government Bill Of Lading Office Code -->
		<!-- IC Comment: (GBLOC) -->
		<!-- IC Comment: 33 Commercial and Government Entity (CAGE) -->
		<!-- IC Comment: [023] Commercial and Government Entity (CAGE) -->
		<!-- IC Comment: [039] Commercial and Government Entity (CAGE) -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='IdentificationCodeQualifier' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='2'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- EDI Indexes: 24, 40 -->
		<!-- IC Comment: Code identifying a party or other code -->
		<!-- IC Comment: [024] Consignee DODAAC/GBLOC/CAGE/D-U-N-S/D-U-N-S+4 -->
		<!-- IC Comment: Carriers are encouraged to provide this data if it is available. It must be provided if identification is not provided at N102. -->
		<!-- IC Comment: [040] Shipper DODAAC/GBLOC/CAGE/D-U-N-S/D-U-N-S+4 -->
		<!-- IC Comment: Carriers are encouraged to provide this data if it is available. It must be provided if identification is not provided at N10 -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='IdentificationCode' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='2'/>
		      <xs:maxLength value='80'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- EDI Indexes: 26, 42 -->
		<!-- IC Comment: Free-form name -->
		<!-- IC Comment: [026] Consignee Additional Name -->
		<!-- IC Comment: [042] Shipper Additional Name -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='PartyAdditionalName' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='60'/>
		    </xs:restriction>
		  </xs:simpleType>
		</xs:element>

		<!-- EDI Indexes: 28, 44 -->
		<!-- IC Comment: Address information -->
		<!-- IC Comment: [028] Consignee Address Information -->
		<!-- IC Comment: [044] Shipper Address Information -->
		<!-- Mapping Comment: Store As-Is -->
		<xs:element name='AddressLineText' minOccurs='0'>
		  <xs:simpleType>
		    <xs:restriction base='xs:string'>
		      <xs:minLength value='1'/>
		      <xs:maxLength value='55'/>
