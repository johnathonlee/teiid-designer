<plugin>
   <extension-point id="advisorStatusProvider" name="%advisorStatusProvider.extension-point.name" schema="schema/advisorStatusProvider.exsd"/>
   <extension-point id="advisorSupplier" name="%advisorSupplier.extension-point.name" schema="schema/advisorSupplier.exsd"/>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="com.metamatrix.modeler.ui"
            class="org.teiid.designer.advisor.ui.views.DSPAdvisorView"
            icon="icons/full/cview16/DSPAdvisor.png"
            id="org.teiid.designer.advisor.ui.views.DSPAdvisorView"
            name="%dspadvisorview.name">
      </view>
   </extension>
    <!-- Extension to allow plugins to contribute to the Modeler Perspective. -->
   <extension
         id="advisorPerspectiveContributor"
         name="%advisorPerspectiveContributor.name"
         point="org.teiid.designer.ui.modelerPerspectiveContributor">
      <class
        name="org.teiid.designer.advisor.ui.views.AdvisorPerspectiveContributor">
      </class>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="com.metamatrix.modeler.ui"
            class="org.teiid.designer.advisor.ui.views.ActionLibraryView"
            icon="icons/full/cview16/AdvisorIcon.png"
            id="org.teiid.designer.advisor.ui.views.ActionLibraryViewID"
            name="Action Library">
      </view>
   </extension>
   
   <extension
         point="org.eclipse.ui.cheatsheets.cheatSheetContent">
      <category
            id="org.teiid.designer.ui.category"
            name="Designer">
      </category>
      <cheatsheet
            category="org.teiid.designer.ui.category"
            composite="false"
            contentFile="$nl$/cheatsheets/New Model From JDBC Import.xml"
            id="org.teiid.designer.ui.cheatsheet_advisor_01"
            name="Model From JDBC Source">
         <description>
            Create Relational Model from JDBC Import
         </description>
      </cheatsheet>
      <cheatsheet
            category="org.teiid.designer.ui.category"
            composite="false"
            contentFile="$nl$/cheatsheets/subsheets/import-from-flat-file.xml"
            id="org.teiid.designer.ui.cheatsheet_advisor_02"
            name="Model Flat File Source">
         <description>
            Model and join two data sources
         </description>
      </cheatsheet>
      <cheatsheet
            category="org.teiid.designer.ui.category"
            composite="false"
            contentFile="$nl$/cheatsheets/Consume SOAP Web Service.xml"
            id="org.teiid.designer.ui.cheatsheet_advisor_03"
            name="Consume a SOAP Web Service">
         <description>
            Consume a SOAP Web Service by creating source and view models based on target WSDL file definition
         </description>
      </cheatsheet>

            <cheatsheet
            category="org.teiid.designer.ui.category"
            composite="true"
            contentFile="$nl$/cheatsheets/Multi-source VDB.xml"
            id="org.teiid.designer.ui.cheatsheet_advisor_04"
            name="Multi-source VDB">
         <description>
            Create and test a VDB joining two data sources
         </description>
      </cheatsheet>
   </extension>
