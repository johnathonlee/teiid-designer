<plugin>

    
<!-- Adding the Mapping Diagram Type extension-->
   <extension
         id="mappingDiagramType"
         name="%mappingDiagramType"
         point="com.metamatrix.modeler.diagram.ui.diagramType">
      <diagram
            class="com.metamatrix.modeler.mapping.ui.diagram.MappingDiagramType"
            simpleDiagram="false">
      </diagram>
   </extension>
<!-- Adding the Mapping Diagram Type extension-->
   <extension
         id="mappingTransformationDiagramType"
         name="%mappingTransformationDiagramType"
         point="com.metamatrix.modeler.diagram.ui.diagramType">
      <diagram
            class="com.metamatrix.modeler.mapping.ui.diagram.MappingDiagramType"
            simpleDiagram="false">
      </diagram>
   </extension>
<!-- Mapping Diagram Type Content And Label Providers -->
   <extension
         id="mappingDiagramContentProvider"
         name="Mapping Diagram Content Provider"
         point="com.metamatrix.modeler.ui.diagramContentProvider">
      <class
            name="com.metamatrix.modeler.mapping.ui.diagram.MappingDiagramContentProvider">
      </class>
      <diagramType
            name="mappingDiagramType">
      </diagramType>
   </extension>
   <extension
         id="mappingDiagramLabelProvider"
         name="Mapping Diagram LabelProvider"
         point="com.metamatrix.modeler.ui.diagramLabelProvider">
      <class
            name="com.metamatrix.modeler.mapping.ui.diagram.MappingDiagramLabelProvider">
      </class>
      <diagramType
            name="mappingDiagramType">
      </diagramType>
   </extension>
   <extension
         id="InputSetObjectEditorPage"
         name="Input Set Object Editor"
         point="com.metamatrix.modeler.ui.modelObjectEditorPage">
      <class
            name="com.metamatrix.modeler.mapping.ui.editor.InputSetObjectEditorPage">
      </class>
   </extension>
   <extension
         id="RecursionObjectEditorPage"
         name="Recursion Object Editor"
         point="com.metamatrix.modeler.ui.modelObjectEditorPage">
      <class
            name="com.metamatrix.modeler.mapping.ui.recursion.RecursionObjectEditorPage">
      </class>
   </extension>
<!-- Mapping Diagram Type Permanent Model Object Action Contributor -->
   <extension
         id="mappingDiagramModelObjectActionContributor"
         point="com.metamatrix.modeler.ui.modelObjectActionContributor">
      <class
            name="com.metamatrix.modeler.mapping.ui.actions.MappingDiagramPermanentActionContributor">
      </class>
   </extension>
   <!-- Helper method to allow external edit for objects. i.e. Delete, Copy, etc.. -->
   <extension
         id="mappingObjectEditHelper"
         name="%mappingObjectEditHelper"
         point="com.metamatrix.modeler.ui.modelObjectEditHelper">
      <class
        name="com.metamatrix.modeler.mapping.ui.actions.MappingObjectEditHelper">
      </class>
   </extension>
   <extension
         point="com.metamatrix.ui.editorPreferences">
         <editorPreferenceComponent
         	class="com.metamatrix.modeler.mapping.ui.preferences.XmlDocumentPreferencesComponent"/>
   </extension>   
 <!-- Helper method to allow external edit for objects. i.e. Delete, Copy, etc.. -->
   <extension
         id="mappingUiNewModelObjectHelper"
         name="%mappingUiNewModelObjectHelper"
         point="com.metamatrix.modeler.core.newModelObjectHelper">
      <class
        name="com.metamatrix.modeler.mapping.ui.util.MappingUiNewModelObjectHelper">
      </class>
   </extension>
<!-- Actions contributed to the object/model generation -->
   <extension
		id="com.metamatrix.modeler.mapping.ui.actions.SetExcludeFromDocumentSpecialAction"
		name="%setExcludeFromDocumentSpecialAction.name"
        point="com.metamatrix.modeler.ui.modelObjectSpecialAction">
      <specialAction
         name="com.metamatrix.modeler.mapping.ui.actions.SetExcludeFromDocumentSpecialAction"
         label="%setExcludeFromDocumentSpecialAction.label">
      </specialAction>
   </extension>
   <extension
		id="com.metamatrix.modeler.mapping.ui.actions.SetIncludeInDocumentSpecialAction"
		name="%setIncludeInDocumentSpecialAction.name"
        point="com.metamatrix.modeler.ui.modelObjectSpecialAction">
      <specialAction
         name="com.metamatrix.modeler.mapping.ui.actions.SetIncludeInDocumentSpecialAction"
         label="%setIncludeInDocumentSpecialAction.label">
      </specialAction>
   </extension>
</plugin>
